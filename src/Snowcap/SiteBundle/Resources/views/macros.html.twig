{% macro project_image_flipper(project, format) %}
    <img
        class="front"
        src="{{ project.thumb_front.webPath | apply_filter(format) | static_path }}"
        alt="{{ project.thumb_front.alt }}"
    />
    <img
        class="back"
        src="{{ project.thumb_back.webPath | apply_filter(format) | static_path }}"
        alt="{{ project.thumb_back.alt }}"
    />
{% endmacro %}
